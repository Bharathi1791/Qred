{
  "target": "es6",
  "compilerOptions": {
    "module": "commonjs",
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "typeRoots": ["src/app/types", "node_modules/@types"],
  },
  "lib": ["es2015"],
  "include": ["src/**/*"]
}